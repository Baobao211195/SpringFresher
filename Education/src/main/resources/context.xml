<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean name="studentRepository" class="edu.fa.repository.StudentRepositoryImpl"></bean>
	
	
	<bean name="studentService" class="edu.fa.service.StudentServiceImpl">
		<!-- Name property mapping with StudentRepository studentRepository in constructor of StudentServiceImpl-->
		<constructor-arg name="studentRepository" ref="studentRepository"></constructor-arg>
	</bean>
</beans>
